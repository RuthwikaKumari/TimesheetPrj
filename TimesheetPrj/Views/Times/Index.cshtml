@model IEnumerable<TimesheetPrj.Models.Time>
@{
    ViewBag.Title = "GetDetails";
    WebGrid grid = new WebGrid(canPage: true, rowsPerPage: 4);
    grid.Bind(source: Model);
}

<h2>GetDetails</h2>

<div class="container">
    <div class="row">
        <div class="col-md-5">
            @grid.GetHtml(
           htmlAttributes: new { @id = "WebGrid1", @class = "table table-bordered table-hover" },
           columns: grid.Columns(
               grid.Column("Id", "Id"),
               grid.Column("Status", "Status"),
               grid.Column("TaskDes", "TaskDescription")
           ))
        </div>
    </div>
</div>
